<% @places.each do |place| %>
  <div class="col-10 offset-1">
    <div class="digg_pagination">
      <%= will_paginate @places %>
    </div>
  </div>
  <div class="booyah-box col-10 offset-1">
    <h1><%= place.name %></h1>
    <i><%= place.address %></i><br />
    <p><%= place.description %></p><br />
  </div>
<% end %>
